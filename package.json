{
  "name": "react-native-range-slider",
  "version": "1.0.6",
  "description": "React native cross platform range slider",
  "main": "src/index",
  "module": "src/index",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "jesster2k10-react-native-range-slider.podspec"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example/ios && node -e \"process.exit(require('os').platform() === 'darwin')\" || pod install",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/jesster2k10/react-native-range-slider/jesster2k10-react-native-range-slider",
  "author": "Jesse Onolememen <jesseono91@gmail.com> (https://github.com/jesster2k10/react-native-range-slider)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesster2k10/react-native-range-slider/jesster2k10-react-native-range-slider/issues"
  },
  "homepage": "https://github.com/jesster2k10/react-native-range-slider/jesster2k10-react-native-range-slider#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/eslint-config": "^0.0.7",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.61.10",
    "commitlint": "^8.3.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "react": "~16.9.0",
    "react-native": "~0.61.5",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
